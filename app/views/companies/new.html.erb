<% provide(:title, 'Entry Comapany') %>
<h1>Entry Company</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@company) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :url %>
      <%= f.text_field :url %>


      <%= f.submit "Entry New Company", class: "btn" %>
    <% end %>
  </div>
</div>